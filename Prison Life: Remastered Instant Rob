local library = loadstring(game:HttpGet(('https://raw.githubusercontent.com/bloodball/-back-ups-for-libs/main/wally2')))()
library.options.underlinecolor = Color3.fromRGB(106, 13, 173)
local w = library:CreateWindow('Prison Life GUI')
local lp = game.Players.LocalPlayer

w:Toggle('Insta Rob', {flag = "instarob"}, function()
	for i,v in pairs(game.Workspace.Map.CITY.JewerlyStore:GetDescendants()) do
		if v.Name == "TouchInterest" then
			v:Destroy()
		end
	end
	for i,v in pairs(game:GetService("Workspace").Map.CITY.Bank.Laser:GetChildren()) do
		if v:FindFirstChild("TouchInterest") then
			v.TouchInterest:Destroy()
		end
	end
end)
spawn(function()
	while wait() do
		if w.flags.instarob then
			wait(0.1)
			spawn(function()
				for i,v in pairs(game:GetService("Workspace").Map.Casino:GetDescendants()) do
					if v.Name == "ProximityPrompt" and (lp:DistanceFromCharacter(v.Parent.Parent.Position) < 7) then
						if lp.Backpack:FindFirstChild("Crowbar") and lp.Character and lp.Character:FindFirstChild("Humanoid") then
							lp.Character.Humanoid:EquipTool(lp.Backpack.Crowbar)
						end
						fireproximityprompt(v)
					end
				end
			end)
			for i,v in pairs(game.Workspace.Map.CITY.ATM:GetDescendants()) do
				if v.Name == "ProximityPrompt" and lp:DistanceFromCharacter(v.Parent.Parent.Position) < 7 then
					if lp.Backpack:FindFirstChild("Crowbar") and lp.Character and lp.Character:FindFirstChild("Humanoid") then
						lp.Character.Humanoid:EquipTool(lp.Backpack.Crowbar)
					end
					fireproximityprompt(v)
				end
			end
			if game.Workspace:FindFirstChild("JewerlyStoreEarning") then
				for i,v in pairs(game.Workspace.JewerlyStoreEarning:GetDescendants()) do
					if v.Name == "ProximityPrompt" and lp:DistanceFromCharacter(v.Parent.Parent.Position) < 15 then
						fireproximityprompt(v)
					end
				end
			end
			for i,v in pairs(game.Workspace.Map.CITY.JewerlyStore:GetDescendants()) do
				if v.Name == "ProximityPrompt" and lp:DistanceFromCharacter(v.Parent.Parent.Position) < 5 then
					if lp.Backpack:FindFirstChild("Crowbar") and lp.Character and lp.Character:FindFirstChild("Humanoid") then
						lp.Character.Humanoid:EquipTool(lp.Backpack.Crowbar)
					end
					fireproximityprompt(v)
				end
			end
			if game.Workspace:FindFirstChild("BankEarning") then
				for i,v in pairs(game:GetService("Workspace").BankEarning:GetDescendants()) do
					if v.Name == "ProximityPrompt" and lp:DistanceFromCharacter(v.Parent.Parent.Position) < 15 then
						fireproximityprompt(v)
					end
				end
			end
			for i,v in pairs(game.Workspace.Map.CITY.Bank:GetDescendants()) do
				if v.Name == "ProximityPrompt" and lp:DistanceFromCharacter(v.Parent.Parent.Position) < 5 then
					if lp.Backpack:FindFirstChild("Crowbar") and lp.Character and lp.Character:FindFirstChild("Humanoid") then
						lp.Character.Humanoid:EquipTool(lp.Backpack.Crowbar)
					end
					fireproximityprompt(v)
				end
			end
		end
	end
end)
game.Workspace.ChildAdded:Connect(function(child)
	spawn(function()
		while w.flags.instarob and child do
			wait(0.1)
			for i,v in pairs(child:GetDescendants()) do
				if v.Name == "ProximityPrompt" then
					fireproximityprompt(v)
				end
			end
		end
	end)
end)
--[[
while wait() do
local bank =  game:GetService("Workspace").Map.CITY.Bank.LED
if not ab and bank.BrickColor == BrickColor.new("Lime Green") then
    ab = true
    	game:GetService("StarterGui"):SetCore("SendNotification",{
				Title = "Bank has reopened",
				Text = "", 
				Duration = 10;
			})
else
    ab = false
    end
end
end)
local jew = game:GetService("Workspace").Map.CITY.JewerlyStore.Main.LED
if not ab and jew.BrickColor == BrickColor.new("Lime Green") then
        ab = true
    	game:GetService("StarterGui"):SetCore("SendNotification",{
				Title = "Jewerly has reopened",
				Text = "", 
				Duration = 10;
			})
    else
     ab = false   
        end
end--]]
