end)
while wait() do
	if not w.flags.ESPTog then
		for i,v in pairs(game.Players:GetPlayers()) do 
			if hasESP(v.Name) then
				removeESP(v.Name)
			else
				activeESPs[v.Name] = nil
			end

		end
	end
end
