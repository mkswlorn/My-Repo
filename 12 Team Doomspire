Button2.Name = "Button2"
Button2.Parent = Frame
Button2.BackgroundColor3 = Color3.new(1, 0, 0)
Button2.BorderSizePixel = 0
Button2.Position = UDim2.new(0.751537085, 0, 0.432098746, 0)
Button2.Size = UDim2.new(0, 18, 0, 21)
Button2.Font = Enum.Font.SourceSans
Button2.Text = ""
Button2.TextColor3 = Color3.new(0, 0, 0)
Button2.TextSize = 14
local Button2On
Button2.MouseButton1Click:Connect(function()
    if not Button2On then
        Button2.BackgroundColor3 = Color3.new(0, 1, 0)
        else
            Button2.BackgroundColor3 = Color3.new(1, 0, 0)
    end
        Button2On = not Button2On
end)
spawn(function()
    while wait(0.2) do 
        pcall(function()
        for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do 
    if Button2On == true and v:IsA("Tool") and (v.Name == "SuperFlyGoldBoombox" or v.Name == "TrololoCapeOfInvisibility" or v.Name == "Acceleration Coil" or v.Name == "GravityCoil") then
    v:Destroy()
            end
        end
        end)
    end
    end)

Text3.Name = "Text3"
