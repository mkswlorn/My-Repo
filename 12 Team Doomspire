Button3.Name = "Button3"
Button3.Parent = Frame
Button3.BackgroundColor3 = Color3.new(1, 0, 0)
Button3.BorderSizePixel = 0
Button3.Position = UDim2.new(0.751537085, 0, 0.604938269, 0)
Button3.Size = UDim2.new(0, 18, 0, 21)
Button3.Font = Enum.Font.SourceSans
Button3.Text = ""
Button3.TextColor3 = Color3.new(0, 0, 0)
Button3.TextSize = 14
local Button3On
Button3.MouseButton1Click:Connect(function()
    if not Button3On then
        Button3.BackgroundColor3 = Color3.new(0, 1, 0)
        local Part = Instance.new("Part", workspace)
        Part.Position = Vector3.new(-11.521, 0.448511, -41.8993)
        Part.Size = Vector3.new(499, 1, 715)
        Part.Anchored = true
        Part.CanCollide = true
        Part.Transparency = 1
        Part.Orientation = Vector3.new(0, 0, 0)
        Part.Name = "Platform"
        else
            Button3.BackgroundColor3 = Color3.new(1, 0, 0)
            if workspace:FindFirstChild("Platform") then
                workspace.Platform:Destroy()
                end
    end
        Button3On = not Button3On
    end)

Hint.Name = "Hint"
Hint.Parent = Frame
Hint.BackgroundColor3 = Color3.new(1, 1, 1)
Hint.BackgroundTransparency = 1
Hint.BorderSizePixel = 0
Hint.Position = UDim2.new(0, 0, 0.790123463, 0)
Hint.Size = UDim2.new(0, 157, 0, 34)
Hint.Font = Enum.Font.SourceSansBold
Hint.Text = "(Hold T and click on a doomspire to teleport below it)"
Hint.TextColor3 = Color3.new(0, 0, 0)
Hint.TextScaled = true
Hint.TextSize = 14
Hint.TextWrapped = true

if workspace:FindFirstChild("Baseplate") and workspace.Baseplate:FindFirstChild("Kill") then
game:GetService("Workspace").Baseplate.Kill:Destroy()
end

local Player = game.Players.LocalPlayer
local Mouse = Player:GetMouse()
local UserInputService = game:GetService('UserInputService')

local HoldingControl = false

Mouse.Button1Down:connect(function()
if HoldingControl and Mouse.Target:IsA("Part") and Mouse.Target:IsDescendantOf(game.Workspace.Doomspires)  then
for i,v in pairs(game.Workspace.Doomspires:GetDescendants()) do 
    if v:IsA("BrickColorValue") and v.Value == Mouse.Target.BrickColor and v.Parent:FindFirstChild("Floor1") then
        Player.Character:MoveTo(v.Parent.Floor1.Stairs:FindFirstChild("StairBrick").Position)
        end
    end
end
end)

UserInputService.InputBegan:connect(function(Input, Processed)
if Input.UserInputType == Enum.UserInputType.Keyboard then
if Input.KeyCode == Enum.KeyCode.T then
HoldingControl = true
end
end
end)

UserInputService.InputEnded:connect(function(Input, Processed)
if Input.UserInputType == Enum.UserInputType.Keyboard then
if Input.KeyCode == Enum.KeyCode.T then
HoldingControl = false
end
end
end)
