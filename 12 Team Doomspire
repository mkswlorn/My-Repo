Frame.Size = UDim2.new(0, 158, 0, 162)
Frame.Active = true
Frame.Draggable = true

Title.Name = "Title"
Title.Parent = Frame
Title.BackgroundColor3 = Color3.new(1, 1, 1)
Title.BorderSizePixel = 0
Title.Size = UDim2.new(0, 157, 0, 28)
Title.Font = Enum.Font.SourceSansBold
Title.Text = "Doomspire Gui"
Title.TextColor3 = Color3.new(0, 0, 0)
Title.TextScaled = true
Title.TextSize = 14
Title.TextWrapped = true

Text1.Name = "Text1"
Text1.Parent = Frame
Text1.BackgroundColor3 = Color3.new(1, 1, 1)
Text1.BorderSizePixel = 0
Text1.Position = UDim2.new(0, 0, 0.259259254, 0)
Text1.Size = UDim2.new(0, 119, 0, 21)
Text1.Font = Enum.Font.SourceSans
Text1.Text = "LuckyBlocks"
Text1.TextColor3 = Color3.new(0, 0, 0)
Text1.TextScaled = true
Text1.TextSize = 14
Text1.TextWrapped = true

Button1.Name = "Button1"
Button1.Parent = Frame
Button1.BackgroundColor3 = Color3.new(1, 0, 0)
Button1.BorderSizePixel = 0
Button1.Position = UDim2.new(0.751537085, 0, 0.259259254, 0)
Button1.Size = UDim2.new(0, 18, 0, 21)
Button1.Font = Enum.Font.SourceSans
Button1.Text = ""
Button1.TextColor3 = Color3.new(0, 0, 0)
Button1.TextSize = 14
local Button1On
Button1.MouseButton1Click:Connect(function()
    if not Button1On then
        Button1.BackgroundColor3 = Color3.new(0, 1, 0)
        for i,v in pairs(workspace:GetChildren()) do 
            if v.Name == "LuckyBlock" then
                firetouchinterest(v.Block, game.Players.LocalPlayer.Character.Head, 0)
                end
            end
        else
            Button1.BackgroundColor3 = Color3.new(1, 0, 0)
    end
        Button1On = not Button1On
    end)
game.Workspace.ChildAdded:Connect(function(a)
    if a.Name == "LuckyBlock" and Button1On == true then
        wait(0.05)
        firetouchinterest(a.Block, game.Players.LocalPlayer.Character.Head, 0)
        end
    end)
Text2.Name = "Text2"
Text2.Parent = Frame
Text2.BackgroundColor3 = Color3.new(1, 1, 1)
Text2.BorderSizePixel = 0
